<?xml version='1.0' encoding='utf-8'?>
<widget id="au.com.ifm.ecusis" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="CDVOrientation">
        <param name="android-package" value="cordova.plugins.screenorientation.CDVOrientation" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>ECUSIS</name>
    <description>
        ECUSIS Map Mobile app
    </description>
    <author email="support@integratedfm.com.au" href="http://integratedfm.com.au">
        IntegratedFM (IFM) Team
    </author>
    <icon src="icon.png" />
    <splash src="icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreenDelay" value="10" />
</widget>
